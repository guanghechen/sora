{
  "name": "@guanghechen/chalk",
  "version": "1.0.0-alpha.4",
  "description": "Chalk (forked from https://github.com/chalk/chalk)",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/sora/tree/@guanghechen/chalk@1.0.0-alpha.4",
    "directory": "packages/chalk"
  },
  "homepage": "https://github.com/guanghechen/sora/tree/@guanghechen/chalk@1.0.0-alpha.4/packages/chalk#readme",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/esm/index.mjs",
      "require": "./lib/cjs/index.cjs",
      "types": "./lib/types/index.d.ts"
    },
    "./node": {
      "source": "./src/node.ts",
      "import": "./lib/esm/node.mjs",
      "require": "./lib/cjs/node.cjs",
      "types": "./lib/types/node.d.ts"
    },
    "./browser": {
      "source": "./src/browser.ts",
      "import": "./lib/esm/browser.mjs",
      "require": "./lib/cjs/browser.cjs",
      "types": "./lib/types/browser.d.ts"
    }
  },
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "types": "./lib/types/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib/ && cross-env NODE_ENV=production rollup -c ../../rollup.config.mjs",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@guanghechen/chalk.types": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "cross-env": "*",
    "rimraf": "*",
    "rollup": "*"
  }
}
